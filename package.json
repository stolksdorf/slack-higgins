{
  "name": "slack-higgins",
  "version": "4.0.0",
  "description": "a slackbot for my personal slack",
  "main": "higgins.js",
  "scripts": {
    "lint": "eslint --fix *.js **/*.{js,jsx}",
    "lint:dry": "eslint *.js **/*.{js,jsx}",
    "start": "node higgins.js",
    "test": "pico-check",
    "test:dev": "pico-check -w -b",
    "dev": "nodemon --verbose --delay 1 higgins.js",
    "sandbox": "nodemon sandbox.js",
    "logs": "heroku logs -t --app=slack-higgins",
    "heroku": "start https://dashboard.heroku.com/apps/slack-higgins",
    "s3": "start https://s3.console.aws.amazon.com/s3/buckets/coolsville-markov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stolksdorf/slack-higgins.git"
  },
  "keywords": [
    "slack"
  ],
  "engines": {
    "node": "^15.0.0"
  },
  "author": "stolksdorf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stolksdorf/slack-higgins/issues"
  },
  "homepage": "https://github.com/stolksdorf/slack-higgins#readme",
  "dependencies": {
    "aws-sdk": "^2.437.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "google": "^1.4.0",
    "google-it": "^1.5.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "node-fetch": "^2.3.0",
    "node-schedule": "^1.3.1",
    "nsapi": "^0.1.15",
    "pg": "^7.7.1",
    "pico-conf": "^1.5.1",
    "pico-gist": "^1.0.1",
    "pico-redis": "^1.0.0",
    "pico-slack": "^2.4.2",
    "pos": "^0.4.2",
    "rcon": "^1.0.3",
    "request": "^2.88.0",
    "sequelize": "^4.41.2",
    "speakeasy-nlp": "^0.2.13",
    "string-similarity": "^1.2.2",
    "superagent": "^3.8.3",
    "syllable": "^3.2.0",
    "xkcd": "^1.1.1",
    "xml2js": "^0.4.19"
